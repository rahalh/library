{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties": {
      "ApplicationName": "Blob API"
    }
  },

  "AWS": {
    "Profile": "default",
    "S3": {
      "StorageDomain": "domain",
      "Prefix": "",
      "BucketName": "blob"
    },
    "DDB": {
      "TableName": "Blob",
      "PartitionKey": "Id",
      "AllowScan": false
    }
  }
}
